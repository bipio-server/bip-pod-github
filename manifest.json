          "name": {
            "type": "string",
            "title": "Name"
          },
          "description": {
            "type": "string",
            "title": "Description"
          },
          "private": {
            "type": "booelan",
            "title": "Is Private"
          },
          "url": {
            "type": "string",
            "title": "URL"
          },
          "html_url": {
            "type": "string",
            "title": "Site URL"
          },
          "watchers_count": {
            "type": "integer",
            "title": "# Watchers"
          },
          "stargazers_count": {
            "type": "integer",
            "title": "# Stargazers"
          },
          "open_issues_count": {
            "type": "integer",
            "title": "# Issues"
          }
        }
      },
      "imports": {
        "properties": {
          "owner": {
            "type": "string",
            "title": "Owner Name"
