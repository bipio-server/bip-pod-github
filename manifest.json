{
  "name": "github",
  "title": "Github",
  "description": "GitHub is a web-based hosting service for software development projects that use the Git revision control system",
  "url": "https://github.com",
  "trackDuplicates": false,
  "config": {
    "oauth": {
      "clientID": "",
      "clientSecret": "",
      "callbackURL": "/rpc/oauth/github/cb",
      "scopes": [
        "gist",
        "notifications",
        "repo:status",
        "repo",
        "read:org"
      ]
    }
  },
  "auth": {
    "strategy": "oauth",
    "properties": {
      "access_token": {
        "type": "string",
        "title": "Access Token"
      }
    },
    "disposition": [
      "access_token"
    ]
  },
  "rpcs": {},
  "actions": {
    "issue_create": {
      "title": "Create an Issue",
      "description": "Create an Issue for the target repository",
      "doc_ref": "https://developer.github.com/v3/issues/#create-an-issue",
      "trigger": "invoke",
      "config": {
        "properties": {
          "owner": {
            "type": "string",
            "title": "Owner Name"
          },
          "repository": {
            "type": "string",
            "title": "Repository Name"
          }
        },
        "disposition": [
          "owner",
          "repository"
        ]
      },
      "exports": {
        "properties": {
          "url": {
            "type": "string",
            "title": "URL"
          },
          "html_url": {
            "type": "string",
            "title": "HTML URL"
          },
          "number": {
            "type": "string",
            "title": "Number"
          },
          "title": {
            "type": "string",
            "title": "Title"
          },
          "body": {
            "type": "string",
            "title": "Body"
          },
          "created_at": {
            "type": "string",
            "title": "Created At"
          }
        }
      },
      "imports": {
        "properties": {
          "title": {
            "type": "string",
            "title": "Title"
          },
          "body": {
            "type": "string",
            "title": "Body"
          }
        },
        "required": [
          "title",
          "body"
        ],
        "disposition": [
          "title",
          "body"
        ]
      }
    },
    "new_user_repos": {
      "disabled": true,
      "title": "User Repository Created",
      "description": "A new User Repository has been created",
      "doc_ref": "",
      "trigger": "poll",
      "config": {
        "properties": {
          "owner": {
            "type": "string",
            "title": "Owner Name, leave blank to track your own repositories"
          },
          "public_only": {
            "type": "boolean",
            "default": true,
            "title": "Public Repositories only"
          }
        },
        "disposition": [
          "owner",
          "public_only"
        ]
      },
      "exports": {
        "properties": {
          "id": {
            "type": "string",
            "title": "ID"
          },
          "name": {
            "type": "string",
            "title": "Name"
          },
          "description": {
            "type": "string",
            "title": "Description"
          },
          "private": {
            "type": "booelan",
            "title": "Is Private"
          },
          "url": {
            "type": "string",
            "title": "URL"
          },
          "html_url": {
            "type": "string",
            "title": "Site URL"
          },
          "watchers_count": {
            "type": "integer",
            "title": "# Watchers"
          },
          "stargazers_count": {
            "type": "integer",
            "title": "# Stargazers"
          },
          "open_issues_count": {
            "type": "integer",
            "title": "# Issues"
          }
        }
      },
      "imports": {
        "properties": {}
      }
    },
    "new_org_repos": {
      "title": "Organization Repository Created",
      "description": "A new Organization Repository has been created",
      "doc_ref": "",
      "trigger": "poll",
      "config": {
        "properties": {
          "organization": {
            "type": "string",
            "title": "Organization Name"
          },
          "public_only": {
            "type": "boolean",
            "default": true,
            "title": "Public Repositories only"
          }
        },
        "required": [
          "organization"
        ],        
        "disposition": [
          "organization",
          "public_only"
        ]
      },
      "exports": {
        "properties": {
          "organization": {
            "type": "string",
            "title": "Organization Name"
          },
          "id": {
            "type": "string",
            "title": "ID"
          },
          "name": {
            "type": "string",
            "title": "Name"
          },
          "description": {
            "type": "string",
            "title": "Description"
          },
          "private": {
            "type": "booelan",
            "title": "Is Private"
          },
          "url": {
            "type": "string",
            "title": "URL"
          },
          "html_url": {
            "type": "string",
            "title": "Site URL"
          },
          "watchers_count": {
            "type": "integer",
            "title": "# Watchers"
          },
          "stargazers_count": {
            "type": "integer",
            "title": "# Stargazers"
          },
          "open_issues_count": {
            "type": "integer",
            "title": "# Issues"
          }
        }
      },
      "imports": {
        "properties": {}
      }
    },
